<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" version="2.0">
    <persistence-unit name="redbullpersistance">
        <class>model.Commande</class>
        <class>model.Reservation</class>
        <properties>
            <property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5432/redbull" />
            <property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver" />
            <property name="javax.persistence.jdbc.user" value="pascal" />
            <property name="javax.persistence.jdbc.password" value="pascal" />
        </properties>
    </persistence-unit>
</persistence>
